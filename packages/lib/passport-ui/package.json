{
  "name": "@pcd/passport-ui",
  "version": "0.10.0",
  "license": "GPL-3.0-or-later",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "files": [
    "dist",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "build": "yarn build:ts && yarn build:types",
    "build:ts": "tsup src/index.ts --format cjs,esm --clean",
    "build:types": "rm -f tsconfig.build.tsbuildinfo && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn clean && yarn build",
    "test": "ts-mocha --config ../../../.mocharc.js --exit test/**/*.spec.ts",
    "clean": "rm -rf dist node_modules",
    "convert-icons": "svgr --out-dir src/icons --typescript icons"
  },
  "dependencies": {
    "medium-zoom": "^1.0.8",
    "pako": "^2.1.0",
    "qr-image": "^3.2.0",
    "react": "^18.2.0",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@pcd/eslint-config-custom": "*",
    "@pcd/tsconfig": "*",
    "@svgr/cli": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/pako": "^2.0.0",
    "@types/qr-image": "^3.2.7",
    "@types/react": "^18.0.22",
    "@types/styled-components": "^5.1.26",
    "eslint": "^7.32.0",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
