{
  "name": "@pcd/zk-eddsa-event-ticket-pcd",
  "version": "0.2.1",
  "license": "GPL-3.0-or-later",
  "main": "./dist/node/index.js",
  "module": "./dist/node/index.mjs",
  "types": "./dist/node/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "types": "./dist/node/index.d.ts",
        "import": "./dist/node/index.mjs",
        "require": "./dist/node/index.js"
      },
      "browser": {
        "types": "./dist/browser/index.d.ts",
        "import": "./dist/browser/index.mjs",
        "require": "./dist/browser/index.js"
      }
    }
  },
  "files": [
    "./artifacts/*",
    "dist",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "build": "tsup src/index.ts --platform=browser --out-dir=./dist/browser --format cjs,esm --dts --clean && tsup src/index.ts --platform=node --out-dir=./dist/node --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --watch",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn build",
    "test": "ts-mocha --config ../../.mocharc.js --exit test/**/*.spec.ts",
    "clean": "rm -rf dist node_modules",
    "artifacts:generate": "pcd-artifacts generate && cp artifacts/* ../../apps/passport-client/public/artifacts/zk-eddsa-event-ticket-pcd && cp artifacts/* ../../apps/passport-server/public/artifacts/zk-eddsa-event-ticket-pcd"
  },
  "dependencies": {
    "@pcd/eddsa-pcd": "0.3.0",
    "@pcd/eddsa-ticket-pcd": "0.3.0",
    "@pcd/passport-ui": "0.8.0",
    "@pcd/pcd-types": "^0.8.0",
    "@pcd/semaphore-identity-pcd": "0.8.0",
    "@pcd/semaphore-signature-pcd": "0.8.0",
    "@pcd/util": "0.2.0",
    "@semaphore-protocol/identity": "^3.14.0",
    "@zk-kit/groth16": "^0.3.0",
    "circomlibjs": "^0.1.7",
    "json-bigint": "^1.0.0",
    "styled-components": "^5.3.9",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pcd/artifacts": "0.2.0",
    "@pcd/eslint-config-custom": "*",
    "@pcd/tsconfig": "*",
    "@types/json-bigint": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "@types/uuid": "^9.0.0",
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
