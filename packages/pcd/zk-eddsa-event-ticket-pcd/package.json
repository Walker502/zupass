{
  "name": "@pcd/zk-eddsa-event-ticket-pcd",
  "version": "0.4.0",
  "license": "GPL-3.0-or-later",
  "main": "./dist/node/index.js",
  "module": "./dist/node/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/browser/index.mjs"
      },
      "node": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/node/index.mjs",
        "require": "./dist/node/index.js"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/browser/index.mjs"
      }
    }
  },
  "files": [
    "./artifacts/*",
    "dist",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "build": "yarn build:ts && yarn build:types",
    "build:ts": "yarn build:browser && yarn build:node",
    "build:browser": "tsup src/index.ts --platform=browser --out-dir=./dist/browser --format esm --clean",
    "build:node": "tsup src/index.ts --platform=node --out-dir=./dist/node --format cjs,esm --clean",
    "build:types": "rm -f tsconfig.tsbuildinfo && tsc --emitDeclarationOnly",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn build && mkdir -p artifacts && cp src/artifacts/* ./artifacts/*",
    "test": "ts-mocha --config ../../../.mocharc.js --exit test/**/*.spec.ts",
    "clean": "rm -rf dist node_modules",
    "artifacts:generate": "pcd-artifacts generate && cp artifacts/* ../../apps/passport-client/public/artifacts/zk-eddsa-event-ticket-pcd && cp artifacts/* ../../apps/passport-server/public/artifacts/zk-eddsa-event-ticket-pcd && cp -f ./artifacts/circuit.json ./src/circuit.json"
  },
  "dependencies": {
    "@pcd/eddsa-pcd": "0.5.0",
    "@pcd/eddsa-ticket-pcd": "0.5.0",
    "@pcd/pcd-types": "^0.10.0",
    "@pcd/semaphore-identity-pcd": "0.10.0",
    "@pcd/semaphore-signature-pcd": "0.10.0",
    "@pcd/util": "0.4.0",
    "@semaphore-protocol/identity": "^3.14.0",
    "@zk-kit/groth16": "^0.3.0",
    "circomlibjs": "^0.1.7",
    "json-bigint": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pcd/artifacts": "0.4.0",
    "@pcd/eslint-config-custom": "*",
    "@pcd/tsconfig": "*",
    "@types/json-bigint": "^1.0.1",
    "@types/mocha": "^10.0.1",
    "@types/uuid": "^9.0.0",
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
