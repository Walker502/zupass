{
  "name": "@pcd/webauthn-pcd",
  "version": "0.10.0",
  "license": "GPL-3.0-or-later",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "build": "yarn build:ts && yarn build:types",
    "build:ts": "tsup src/index.ts --format cjs,esm --clean",
    "build:types": "rm -f tsconfig.build.tsbuildinfo && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn clean && yarn build",
    "test": "jest",
    "clean": "rm -rf dist node_modules"
  },
  "files": [
    "dist",
    "./README.md",
    "./LICENSE"
  ],
  "dependencies": {
    "@pcd/passport-crypto": "0.10.0",
    "@pcd/pcd-types": "0.10.0",
    "@pcd/util": "0.4.0",
    "@simplewebauthn/browser": "^7.2.0",
    "@simplewebauthn/server": "^7.2.0",
    "@simplewebauthn/typescript-types": "^7.0.0",
    "json-bigint": "^1.0.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pcd/eslint-config-custom": "*",
    "@pcd/tsconfig": "*",
    "@types/jest": "^29.5.0",
    "@types/json-bigint": "^1.0.1",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
