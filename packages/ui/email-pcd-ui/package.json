{
  "name": "@pcd/email-pcd-ui",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "build": "concurrently \"yarn build:ts\" \"yarn build:types\"",
    "build:ts": "tsup src/index.ts --format cjs,esm --clean",
    "build:types": "rm -rf dist/types && tsc --emitDeclarationOnly --outDir dist/types/src",
    "dev": "concurrently \"yarn dev:ts\" \"yarn dev:types\"",
    "dev:ts": "tsup src/index.ts --format cjs,esm --watch",
    "dev:types": "tsc --emitDeclarationOnly --outDir dist/types/src --watch",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn build",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@pcd/email-pcd": "0.5.0",
    "@pcd/passport-ui": "0.10.0",
    "@pcd/pcd-types": "0.10.0"
  },
  "devDependencies": {
    "@pcd/eslint-config-custom": "*",
    "@pcd/tsconfig": "*",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "eslint": "^7.32.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
