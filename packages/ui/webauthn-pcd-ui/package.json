{
  "name": "@pcd/webauthn-pcd-ui",
  "version": "0.1.0",
  "license": "GPL-3.0-or-later",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "files": [
    "dist",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "lint": "eslint \"**/*.ts{,x}\"",
    "build": "yarn build:ts && yarn build:types",
    "build:ts": "tsup src/index.ts --format cjs,esm --clean",
    "build:types": "rm -f tsconfig.build.tsbuildinfo && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "typecheck": "yarn tsc --noEmit",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@pcd/webauthn-pcd": "0.10.0",
    "@pcd/passport-ui": "0.10.0",
    "@pcd/pcd-types": "0.10.0"
  },
  "devDependencies": {
    "@pcd/eslint-config-custom": "*",
    "@pcd/tsconfig": "*",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "eslint": "^7.32.0",
    "tsup": "^6.7.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
